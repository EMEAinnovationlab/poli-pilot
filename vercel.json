{
  "version": 2,
  "functions": {
    "api/index.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "^/(chat|site-content|project-settings|health)$", "dest": "/api/index.js" },
    { "src": "^/(auth|admin|documents)(/.*)?$", "dest": "/api/index.js" },

    { "handle": "filesystem" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^(?!/api/).*$", "dest": "/$1" }
  ]
}
