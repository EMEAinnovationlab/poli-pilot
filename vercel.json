{
  "version": 2,
  "routes": [
    { "src": "^/(auth|documents)(?:/.*)?$", "dest": "/api/index.js" },
    { "src": "^/(chat|project-settings|site-content|example-prompts)$", "dest": "/api/index.js" },

    { "src": "^/api/(auth|documents)(?:/.*)?$", "dest": "/api/index.js" },
    { "src": "^/api/(chat|project-settings|site-content|example-prompts)$", "dest": "/api/index.js" },

    { "src": "^/(js|css|images)/(.*)$", "dest": "/public/$1/$2" }
  ]
}
