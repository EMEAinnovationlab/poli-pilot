{
  "version": 2,
  "routes": [
    { "src": "^/api/.*$", "dest": "/api/index.js" },

    { "src": "^/(auth|admin|chat|documents|site-content|project-settings)(/.*)?$", "dest": "/api/index.js" },
    { "src": "^/health$", "dest": "/api/index.js" },

    { "handle": "filesystem" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^(?!/api/).*$", "dest": "/$1" }
  ]
}
